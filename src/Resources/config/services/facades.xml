<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="CheckoutCom\Shopware6\Facade\PaymentPayFacade">
            <argument type="service" id="checkout_com.shopware6.logger"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="CheckoutCom\Shopware6\Factory\SettingsFactory"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CheckoutApi\CheckoutPaymentService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CustomerService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Extractor\OrderExtractor"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderTransactionService"/>
            <argument type="service" id="router"/>
        </service>

        <service id="CheckoutCom\Shopware6\Facade\PaymentFinalizeFacade">
            <argument type="service" id="checkout_com.shopware6.logger"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CheckoutApi\CheckoutPaymentService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Factory\SettingsFactory"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CustomerService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Extractor\OrderExtractor"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderTransactionService"/>
        </service>

        <service id="CheckoutCom\Shopware6\Facade\DirectPayFacade">
            <argument type="service" id="router"/>
            <argument type="service" id="Shopware\Core\Checkout\Payment\PaymentService"/>
            <argument type="service" id="checkout_com.shopware6.logger"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\ContextService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Cart\CartService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Cart\CartBackupService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\ShippingMethodService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\PaymentMethodService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CountryService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CustomerService"/>
        </service>

        <service id="CheckoutCom\Shopware6\Facade\PaymentRefundFacade">
            <argument type="service" id="checkout_com.shopware6.logger"/>
            <argument type="service" id="Shopware\Core\Checkout\Cart\Order\OrderConverter"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Extractor\OrderExtractor"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Builder\RefundBuilder"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Product\ProductService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Cart\CartService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\Order\OrderTransactionService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\CheckoutApi\CheckoutPaymentService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Service\PaymentMethodService"/>
            <argument type="service" id="CheckoutCom\Shopware6\Factory\SettingsFactory"/>
        </service>
    </services>
</container>
