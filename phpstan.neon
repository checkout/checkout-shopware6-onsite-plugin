parameters:
    paths:
        - src
        - tests
    level: 5
    autoload_files:
        - %rootDir%/../../../vendor/autoload.php
    autoload_directories:
    	- %rootDir%/../../../src
    excludes_analyse:
        - src/Resources/app/storefront/node_modules/
        - src/Resources/app/administration/node_modules/
        - tests/Cypress/
        - %rootDir%/../../../vendor/*
        - easy-coding-standard.php
